#include <stdio.h>

void sort(int c[], int m);

int main(){
    int in = 0;
    scanf("%d", &in);
    int ar[in];

    for (int i = 0; i < in; i++){
        scanf("%d", &ar[i]);
    }

    sort(ar, in);

    for (int i = in - 1; i >= 0; i--){
        if (i == 0){
            printf("%d\n", ar[i]);
        } else {
            printf("%d ", ar[i]);
        }
    }

    return 0;
}

void sort(int c[], int m){
    int b[m];

    for (int j = 0; j < m; j++){
        b[j] = 0;
        int a = c[j];

        while (a > 0){//各位數累加
            b[j] += a % 10;
            a /= 10;
        }
    }

    for (int i = 0; i < m; i++){
        for (int j = 0; j < m; j++){
            if (b[i] > b[j] || (b[i] == b[j] && c[i] > c[j])){
                // Swap
                int tc = c[i];
                c[i] = c[j];
                c[j] = tc;

                int tb = b[i];
                b[i] = b[j];
                b[j] = tb;
            }
        }
    }
}
